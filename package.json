{
  "name": "universal-redux-template",
  "version": "0.0.0",
  "description": "Universal Redux Template",
  "scripts": {
    "start": "cross-env NODE_PATH=./app concurrently --kill-others \"nodemon app/server\" \"node webpack.server.js\" \"gulp css:watch\"",
    "test": "cross-env NODE_ENV=test cross-env NODE_PATH=./app mocha --compilers js:babel-register -r app/spec/support/setup.mocha.js --recursive 'app/spec/**/*.test.js' -w",
    "test:ci": "cross-env NODE_ENV=test cross-env NODE_PATH=./app mocha --compilers js:babel-register -r app/spec/support/setup.mocha.js --recursive 'app/spec/**/*.test.js'",
    "webpack": "webpack",
    "build": "rimraf dist/ && npm run webpack && gulp css:build",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "6.1.0",
    "npm": "3.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mz026/universal-redux-template"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mz026/universal-redux-template/issues"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-register": "^6.5.1",
    "bluebird": "^3.4.6",
    "compression": "^1.6.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "history": "^4.4.0",
    "humps": "^2.0.0",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^4.16.6",
    "normalizr": "^2.2.1",
    "react": "^15.0.2",
    "react-addons-pure-render-mixin": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.0.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "rewire": "^2.3.4",
    "superagent": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-rewire": "^1.0.0-beta-5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "chai": "^3.3.0",
    "chai-as-promised": "^6.0.0",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "enzyme": "^2.3.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rev": "^7.1.2",
    "gulp-sass": "^2.1.1",
    "gulp-sourcemaps": "^2.2.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "node-sass": "^3.4.2",
    "nodemon": "^1.6.0",
    "react-addons-test-utils": "^15.0.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.2",
    "rewire-webpack": "^1.0.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
